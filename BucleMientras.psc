Algoritmo BucleMientras
	// Declaración de variables
	Definir contador Como Entero;
	// Inicialización del contador
    contador <- 1
	// Bucle "Mientras" para imprimir los números del 1 al 10
    Mientras contador <= 10 Hacer
		Escribir contador;
        contador <- contador + 1;
    FinMientras
	
FinAlgoritmo
